{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "jsdoc"
    ],
    "rules": {
        "linebreak-style": "off",
        "valid-jsdoc":"off",
        "require-jsdoc":"off",
        "jsdoc/require-description": 1,
        "jsdoc/require-jsdoc": 1
    }
}
